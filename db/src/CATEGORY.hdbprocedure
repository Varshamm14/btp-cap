CREATE OR REPLACE PROCEDURE CATEGORY (
    IN p_Name NVARCHAR(100),
    IN p_Description NVARCHAR(500),
    IN p_Status NVARCHAR(50)
)
LANGUAGE SQL
AS $$
BEGIN
    -- Insert the provided data into the MafiTrailers table
    INSERT INTO MafiTrailers (Name, Description, Status)
    VALUES (p_Name, p_Description, p_Status);
END;
$$;
