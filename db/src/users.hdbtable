 COLUMN TABLE "users" (
    "id" BIGINT GENERATED BY DEFAULT AS IDENTITY,  -- Auto-generated primary key
    "username" NVARCHAR(256) NOT NULL,            -- Username of the user
    "password" NVARCHAR(256) NOT NULL,            -- Password of the user
    "status" TINYINT DEFAULT 0,                   -- Status of the user (0 for inactive, 1 for active)
    PRIMARY KEY ("id")                            -- Primary key on 'id'
) UNLOAD PRIORITY 5 AUTO MERGE;

--sets the priority for unloading table data from memory (RAM) when memory pressure occurs in the database.
--database automatically merges delta and main stores, improving performance for read-heavy operations and ensuring efficient memory usage.